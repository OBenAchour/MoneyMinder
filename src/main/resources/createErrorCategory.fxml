<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="513.0" prefWidth="500.0" stylesheets="@stylesReclamation.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.errorCategoryController">
   <children>
      <Label layoutX="70.0" layoutY="39.0" styleClass="label-title" text="Add Error Category" textFill="#004b57">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Label layoutX="27.0" layoutY="130.0" styleClass="label-subtitle" text="Name :" textFill="#004b57">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="nameTextField" layoutX="91.0" layoutY="128.0" styleClass="text-field" />
      <Button layoutX="278.0" layoutY="122.0" mnemonicParsing="false" onAction="#addErrorCategory" prefHeight="41.0" prefWidth="194.0" styleClass="button-add" text="Add" />
      <TableView fx:id="errorCategoryTableView" layoutX="26.0" layoutY="173.0" prefHeight="326.0" prefWidth="464.0" styleClass="table-view">
         <columns>
            <TableColumn fx:id="nameColumn" styleClass="table-column-name" text="Name" />
            <TableColumn fx:id="removeColumn" styleClass="table-column-action" text="Action" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
